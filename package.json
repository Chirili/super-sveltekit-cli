{
  "name": "super-sveltekit-cli",
  "version": "0.2.1",
  "description": "Super Sveltekit CLI is a command line tool for the Sveltekit framework. The idea behind this tool is to facilitate the creation of routes and integration of libraries and services.",
  "type": "module",
  "bin": {
    "ssc": "dist/index.js"
  },
  "scripts": {
    "build": "npx tsc && npm run add-templates",
    "dev": "ts-node --esm .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "add-templates": "copyfiles -u -l 1 src/templates/**/* dist"
  },
  "keywords": ["svelte","kit","cli","tool","sveltekit","command","routes","code-generation"],
  "author": "Andr√©s Carmona Lozano",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "copyfiles": "^2.4.1",
    "eta": "^3.1.0",
    "inquirer": "^9.2.10"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.10",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
